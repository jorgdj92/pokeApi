{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "libs/axioslib/(.*)": "<rootDir>/../libs/axioslib/src/$1",
    "libs/axioslib": "<rootDir>/../libs/axioslib/src"
  }
}